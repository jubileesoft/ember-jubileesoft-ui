<Doc as |doc|>
  <doc.element @name="Select">
    <JubileeSelect
      @items={{this.items}}
      @selectedItem={{this.selectedItem}}
      @onChange={{action (mut this.selectedItem)}} as |name|
    >
      {{name}}
    </JubileeSelect>
  </doc.element>
  <doc.controls />
  <doc.hbs @code={{this.code}} />
</Doc>